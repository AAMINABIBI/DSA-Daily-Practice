Maximum Frequency Number

int maximumFrequency(vector<int> &arr, int n)
{ int maxfreq=0;
    int maxans=0; 
   

    unordered_map<int,int> count;
    for(int i=0;i<arr.size();i++)
    {
        count[arr[i]]++;
        maxfreq=max(maxfreq,count[arr[i]]);
    }

    for(int i=0;i<arr.size();i++)
    {
        if(maxfreq==count[arr[i]])
         maxans=arr[i];
    break;
    }
    return maxans;
    //Write your code here
}
